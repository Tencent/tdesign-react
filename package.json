{
  "name": "tdesign-react",
  "purename": "tdesign",
  "version": "0.18.2",
  "description": "TDesign Component for React",
  "title": "tdesign-react",
  "main": "cjs/index.js",
  "module": "es/index.js",
  "typings": "es/index.d.ts",
  "unpkg": "dist/tdesign.min.js",
  "jsdelivr": "dist/tdesign.min.js",
  "files": [
    "esm",
    "es",
    "es/index.d.ts",
    "cjs",
    "lib",
    "dist",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "sideEffects": [
    "dist/*",
    "site/*",
    "es/**/style/**",
    "esm/**/style/**"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "start": "cd site && vite",
    "site": "cd site && vite build",
    "site:serve": "cd site && vite preview",
    "site:staging": "cd site && vite build --mode staging",
    "lint": "eslint ./src --max-warnings 0 --ext .ts,.tsx --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prebuild": "rimraf es/* lib/* dist/* esm/* cjs/*",
    "build": "cross-env NODE_ENV=production rollup -c script/rollup.config.js && npm run build:tsc",
    "build:tsc": "concurrently \"npm:build:tsc-*\"",
    "build:tsc-es": "tsc --emitDeclarationOnly -d --outDir es/",
    "build:tsc-esm": "tsc --emitDeclarationOnly -d --outDir esm/",
    "build:tsc-lib": " tsc --emitDeclarationOnly -d --outDir lib/",
    "build:tsc-cjs": " tsc --emitDeclarationOnly -d --outDir cjs/",
    "changelog": "node script/generate-changelog.js",
    "init": "node script/init-component",
    "robot": "publish-cli robot-msg"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --write",
      "npm run lint",
      "git add"
    ]
  },
  "keywords": [
    "tdesign",
    "react"
  ],
  "author": "tdesign",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.13.1",
    "react-dom": ">=16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-url": "^6.0.0",
    "@testing-library/jest-dom": "^5.9.0",
    "@testing-library/react": "^10.2.0",
    "@types/classnames": "^2.3.1",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/jest": "^25.2.1",
    "@types/lodash": "^4.14.170",
    "@types/node": "^11.15.4",
    "@types/raf": "^3.4.0",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^4.3.5",
    "@types/react-transition-group": "^4.4.2",
    "@types/rimraf": "^3.0.0",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "autoprefixer": "^10.4.0",
    "babel-jest": "^26.0.1",
    "babel-polyfill": "^6.26.0",
    "chalk": "^4.1.0",
    "concurrently": "^6.4.0",
    "cross-env": "^5.2.1",
    "cssnano": "^4.0.1",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "^0.12.16",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "husky": "^3.1.0",
    "jest": "^26.6.3",
    "jest-transform-stub": "^2.0.0",
    "less": "^4.1.1",
    "lint-staged": "^10.1.3",
    "markdown-it-container": "^3.0.0",
    "markdown-it-fence": "^0.1.3",
    "mockdate": "^3.0.5",
    "postcss": "^8.3.11",
    "prettier": "^2.3.2",
    "prismjs": "^1.23.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "rimraf": "^2.7.1",
    "rollup": "^2.55.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-esbuild": "^4.5.0",
    "rollup-plugin-ignore-import": "^1.3.2",
    "rollup-plugin-multi-input": "^1.3.1",
    "rollup-plugin-postcss": "^4.0.1",
    "rollup-plugin-static-import": "^0.1.1",
    "rollup-plugin-styles": "^3.14.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "standard-changelog": "^2.0.27",
    "tdesign-publish-cli": "^0.0.6",
    "tdesign-site-components": "^0.1.2",
    "ts-jest": "^26.5.6",
    "ts-morph": "^12.2.0",
    "ts-node": "^8.8.1",
    "typescript": "^3.9.7",
    "vite": "^2.5.6",
    "vite-plugin-pwa": "^0.11.3",
    "vite-plugin-tdoc": "^1.0.5",
    "workbox-precaching": "^6.3.0"
  },
  "dependencies": {
    "@babel/runtime": "~7.15.4",
    "@popperjs/core": "~2.4.0",
    "classnames": "~2.3.1",
    "clipboard": "~2.0.8",
    "dayjs": "1.9.7",
    "hoist-non-react-statics": "~3.3.2",
    "lodash": "~4.17.15",
    "raf": "~3.4.1",
    "react-popper": "~2.2.3",
    "react-transition-group": "~4.4.1",
    "tdesign-icons-react": "~0.0.5",
    "tslib": "~2.2.0",
    "use-resize-observer": "^8.0.0",
    "uuid": "~8.3.2",
    "validator": "~13.7.0"
  }
}
